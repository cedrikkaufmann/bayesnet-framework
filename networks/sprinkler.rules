sprinkler begin
    if cloudy=true then false
    if cloudy=false then true
end

rainy begin
    if cloudy=true then true
    if cloudy=false then false
end

wetGrass begin
    if sprinkler=true & rainy=true then true
    if sprinkler=false & rainy=true then true
    if sprinkler=true & rainy=false then true
    if sprinkler=false & rainy=false then false
end